#!/bin/bash

java -cp /usr/local/bin/share/histner/stanford-ner.jar edu.stanford.nlp.ie.crf.CRFClassifier -loadClassifier /usr/local/bin/share/histner/hist-model-org.ser.gz -encoding utf-8 -readStdin -tokenizerFactory edu.stanford.nlp.process.WhitespaceTokenizer -tokenizerOptions "tokenizeNLs=true" -outputFormat "tsv"
